{
  "name": "urionlinejudge-cli",
  "version": "0.0.0-semantically-released",
  "description": "The URIOnlineJudge command line utility.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/matheussampaio/urionlinejudge-cli"
  },
  "scripts": {
    "test": "gulp test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "commit-wizard"
  },
  "author": "Matheus Sampaio <matheus@sampaio.us> (http://matheus.sampaio.us)",
  "bugs": {
    "url": "https://github.com/matheussampaio/urionlinejudge-cli/issues"
  },
  "homepage": "https://github.com/matheussampaio/urionlinejudge-cli",
  "license": "MIT",
  "preferGlobal": true,
  "bin": {
    "urionlinejudge": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "cli",
    "urionlinejudge"
  ],
  "dependencies": {
    "chalk": "1.1.1",
    "lodash": "3.10.1",
    "phantom": "0.8.3",
    "progress": "1.1.8",
    "read": "1.0.7",
    "yargs": "3.30.0"
  },
  "devDependencies": {
    "babel-core": "6.2.1",
    "babel-preset-es2015": "6.1.18",
    "chai": "3.4.1",
    "del": "2.1.0",
    "eslint-config-airbnb": "1.0.0",
    "gulp": "3.9.0",
    "gulp-babel": "6.1.0",
    "gulp-changed": "1.3.0",
    "gulp-esdoc": "0.2.0",
    "gulp-eslint": "1.1.0",
    "gulp-if": "2.0.0",
    "gulp-load-plugins": "1.1.0",
    "gulp-mocha": "2.2.0",
    "gulp-plumber": "1.0.1",
    "gulp-util": "3.0.7",
    "pre-git": "1.3.1",
    "run-sequence": "1.1.5",
    "semantic-release": "4.3.5",
    "validate-commit-msg": "1.0.0"
  },
  "config": {
    "pre-git": {
      "commit-msg": "validate-commit-msg",
      "pre-push": [
        "rm -rf node_modules",
        "npm install",
        "gulp build",
        "gulp test"
      ],
      "pre-commit": [],
      "post-commit": [],
      "post-merge": []
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
